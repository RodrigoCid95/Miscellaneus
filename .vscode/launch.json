{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wails: Production app",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/app/build/bin/Miscellaneous.exe",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Debug app",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/app/build/bin/Miscellaneous.exe",
      "preLaunchTask": "build debug",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Dev app",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/app/build/bin/Miscellaneous.exe",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/server/main.go"
    }
  ]
}